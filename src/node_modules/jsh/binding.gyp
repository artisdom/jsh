{
  "targets": [
    {
      "target_name": "jsh",
      "sources": [ "jsh.cpp" ],
      "cflags_cc": [ "-std=c++0x" ],
      "include_dirs": [ "../common", "<!(node -e \"require('nan')\")" ],
      'conditions': [
        [ 'OS=="mac"', {
          'xcode_settings': {
            'MACOSX_DEPLOYMENT_TARGET': '10.7',
            'OTHER_CPLUSPLUSFLAGS' : ['-std=c++11','-stdlib=libc++'],
            'OTHER_LDFLAGS': ['-stdlib=libc++'],
	    },
	}],
      ]
    }
  ]
}
